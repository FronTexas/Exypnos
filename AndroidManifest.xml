        <activity
            android:name="com.frontexas.exypnos.Homepage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name=".Homepage" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.frontexas.exypnos.SearchPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name=".SearchPage" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.frontexas.exypnos.DoctorProfilePage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name=".DoctorProfilePage" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>