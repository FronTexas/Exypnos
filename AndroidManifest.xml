            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.frontexas.exypnos.ExypnosDrawerActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name=".ExypnosDrawer" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.frontexas.exypnos.SearchResultsPage"
            android:label="@string/app_name" >
